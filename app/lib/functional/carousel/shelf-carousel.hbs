<div class="carousel-shelf {{modifier_class}}" itemprop="offers">
  <div class="carousel-item">
    <div class="item-box">
      <a href="/url/de/producto/" title="Título de producto">
        {{!-- Elemento contenedor de imagenes --}}
        <div class="img-container">
          {{!-- Esta imágen está puesta solamente como demostración. --}}
